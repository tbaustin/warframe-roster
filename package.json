{
	"name": "warframe-roster",
	"version": "1.0.0",
	"author": "Taylor Austin <tbaustin1992@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/escaladesports/warframe-roster.git"
	},
	"scripts": {
		"build": "npm run build:gatsby",
		"build:lambda": "netlify-lambda build src/lambda --config lambda-webpack-config.js",
		"build:gatsby": "npm run build:lambda && NODE_ENV=production gatsby build",
		"build:module": "rm -rf dist && mkdir dist && babel src/export --out-dir dist",
		"build:plugin": "babel plugins/export --out-dir ./",
		"clean": "run-p clean:cache clean:public clean:functions",
		"clean:cache": "rm -rf .cache",
		"clean:public": "rm -rf public",
		"clean:functions": "rm -rf dist-functions",
		"dev": "run-p dev:functions dev:gatsby",
		"dev:gatsby": "gatsby develop --https --open",
		"dev:functions": "netlify-lambda serve src/lambda --config lambda-webpack-config.js",
		"env": "node utils/env",
		"lint": "eslint ./",
		"live": "opn https://warframe-roster.netlify.com/",
		"netlify": "opn https://app.netlify.com/sites/warframe-roster/deploys",
		"precommit": "lint-staged",
		"prepublishOnly": "npm run build",
		"reset": "node utils/reset",
		"serve": "gatsby serve --open",
		"sig": "node src/functions/test-signature",
		"test": "jest",
		"travis": "opn https://travis-ci.org/escaladesports/warframe-roster/branches"
	},
	"jest": {
		"testRegex": "/.*(__tests__\\/.*)|(.*(test|spec))\\.jsx?$",
		"moduleNameMapper": {
			".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
			".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
		},
		"setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
		"testPathIgnorePatterns": [
			"node_modules",
			".cache",
			"__snapshots__",
			"public",
			"dist"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!(gatsby)/)"
		],
		"globals": {
			"__PATH_PREFIX__": ""
		},
		"testURL": "http://localhost",
		"setupFiles": [
			"<rootDir>/loadershim.js"
		],
		"coverageDirectory": "<rootDir>/public/coverage"
	},
	"lint-staged": {
		"**/*.js": [
			"eslint --fix",
			"git add"
		]
	},
	"dependencies": {
		"@babel/runtime": "^7.0.0",
		"eslint-config-prettier": "^4.0.0"
	},
	"main": "dist/index.js",
