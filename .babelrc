{
	"plugins": [
		["module-resolver", {
			"root": ["."],
			"alias": {
				"styles": "./styles",
				"config": "./config",
				"json": "./json",
				"utils": "./utils"
			},
			"cwd": "babelrc"
		}],
		["styled-components", {
			"ssr": true,
			"displayName": true,
			"preprocess": false
		}],
		["wrap-in-js", { "extensions": ["css$"] }],
		"inline-react-svg"
	],
	"presets": [
		["next/babel", {
			"styled-jsx": {
				"plugins": [
					"styled-jsx-plugin-postcss"
				]
			}
		}]
	],
	"ignore": []
}
